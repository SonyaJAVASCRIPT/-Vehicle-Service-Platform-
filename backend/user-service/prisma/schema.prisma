generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:133755@localhost:5432/vehicleServicePlatform?schema=public"
}

model user {
  id       Int    @id @default(autoincrement())
  username String
  email    String @unique
}
